USE [CORPORERM]
GO

SET ANSI_NULLS ON
GO

SET QUOTED_IDENTIFIER ON
GO

CREATE TABLE [dbo].[ZCONSTANTES]
(
	[TABELA] [nvarchar](250) NOT NULL,
	[CAMPO] [nvarchar](250) NOT NULL,
	[CODSTATUS] [nvarchar](50) NOT NULL,
	[STATUS] [nvarchar](250) NULL,
	[OBSERVACAO] [nvarchar](1000) NULL,
	CONSTRAINT [PKZCONSTANTES] PRIMARY KEY CLUSTERED 
	(
		 [TABELA] ASC
		,[CAMPO] ASC
		,[CODSTATUS] ASC
	)
	WITH 
	(
		 PAD_INDEX = OFF
		,STATISTICS_NORECOMPUTE = OFF
		,IGNORE_DUP_KEY = OFF
		,ALLOW_ROW_LOCKS = ON
		,ALLOW_PAGE_LOCKS = ON
		,FILLFACTOR = 99
		,OPTIMIZE_FOR_SEQUENTIAL_KEY = OFF
		,DATA_COMPRESSION = PAGE
	) ON [PRIMARY]
) ON [PRIMARY]
GO


